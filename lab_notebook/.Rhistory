setwd("~/current_research/CPOM_Flux/lab_notebook")
cn.ll <- read.table("./data/leached_litter_CN.csv", header = T, sep = ",")
# CPOM Flux exp
cn.cf <- read.table("./data/CPOM_Flux_CN.csv", header = T, sep = ",")
cn.ll <- cn.ll[cn.ll$Repeat == "N", ]
cor(percN ~ percC, data = cn.cf, subset = Source == "leaf")
cor(cn.cf$percN[cn.cf$Source == "Leaf"], cn.cf$percC[cn.cf$Source == "Leaf"])
cor(cn.cf$percN[cn.cf$Source == "Leaf"], cn.cf$percC[cn.cf$Source == "Leaf"], na.rm = T)
cor.test(percN ~ percC, data = cn.cf, subset = Source == "leaf", main = "CPOM Flux Exp")
plot(percN ~ percC, data = cn.cf, subset = Source == "leaf", main = "CPOM Flux Exp")
cor.test(percN ~ percC, data = cn.cf, subset = Source == "leaf")
summary(lm(percN ~ percC, data = cn.cf, subset = Source == "leaf"))
summary(lm(percN ~ percC, data = cn.ll, subset = Type == "Leaf", main = "Leached Litter Exp"))
summary(lm(percN ~ percC, data = cn.ll, subset = Type == "Leaf"))
par(las = 1)
plot(percN ~ percC, data = cn.ll, subset = Type == "Leaf", main = "Leached Litter Exp")
dev.copy(jpeg, "./output/plots/percC_by_percN_leached_litter.jpg")
